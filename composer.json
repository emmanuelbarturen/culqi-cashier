    }
  },
  "extra": {
    "branch-version": "4.2-dev",
    "laravel": {
      "providers": [
        "Emm\\CulqiCashier\\CulqiCashierServiceProvider"
      ],
      "aliases": {
        "CulqiCashier": "Emm\\Uuid\\Uuid"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
